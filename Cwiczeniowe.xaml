        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RoomsFinderWsiiz"
        mc:Ignorable="d"
        Title="Cwiczeniowe" Height="768" Width="1024" Background="Black" WindowState="Maximized" WindowStartupLocation="CenterScreen" WindowStyle="None">
    <Grid x:Name="kolor">
        <MediaElement x:Name="filmc" HorizontalAlignment="Center" Height="551" Margin="10,10,10,202" VerticalAlignment="Center" Width="999" LoadedBehavior="Manual" UnloadedBehavior="Manual"/>
        <Button x:Name="ra17" Content="RA17" HorizontalAlignment="Left" Margin="439,586,0,0" VerticalAlignment="Top" Width="75" Click="Ra17_Click"/>
        <Button x:Name="exit" Content="" HorizontalAlignment="Left" Height="140" Margin="10,586,0,0" VerticalAlignment="Center" Width="153" BorderBrush="{x:Null}" Foreground="{x:Null}" Click="Exit_Click">
            <Button.Background>
                <ImageBrush ImageSource="Pliki/projekt/back-arrow.png"/>
            </Button.Background>
        </Button>

    </Grid>
</Window>
